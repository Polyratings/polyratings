{
    "name": "eslint-config",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/eslint-config/src",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "packages/eslint-config/**/*.ts",
                    "packages/eslint-config/**/*.js"
                ]
            }
        },
        "lint-fix": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "packages/eslint-config/**/*.ts",
                    "packages/eslint-config/**/*.js"
                ],
                "fix": true
            }
        }
    },
    "tags": []
}
