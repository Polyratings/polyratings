{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build:fast",
          "build",
          "lint",
          "fix",
          "build:local",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build:fast": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build:local": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
